{
  "name": "Personal Extension",
  "description": "nice",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "tabs", "scripting",  "contextMenus", "storage", "history", "bookmarks"],
  "host_permissions": [
    "http://*/*", "https://*/*"
  ],
  "action": {
    "default_popup": "index.html"
  }
  
}

